<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.20.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="untitled1">
        <createTable tableName="CHILD">
            <column name="ID" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_CHILD"/>
            </column>
            <column name="NAME" type="VARCHAR(255)"/>
            <column name="AGE" type="INT"/>
            <column name="PARENT_ID" type="VARCHAR(255)"/>
            <column name="VERSION" type="INT">
                <constraints nullable="false"/>
            </column>
            <column name="CREATED_BY" type="VARCHAR(255)"/>
            <column name="CREATED_DATE" type="${offsetDateTime.type}"/>
            <column name="LAST_MODIFIED_BY" type="VARCHAR(255)"/>
            <column name="LAST_MODIFIED_DATE" type="${offsetDateTime.type}"/>
            <column name="DELETED_BY" type="VARCHAR(255)"/>
            <column name="DELETED_DATE" type="${offsetDateTime.type}"/>
        </createTable>
    </changeSet>
    <changeSet id="2" author="untitled1">
        <createTable tableName="EMP">
            <column name="ID" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_EMP"/>
            </column>
            <column name="NAME" type="VARCHAR(255)"/>
            <column name="AGE" type="INT"/>
            <column name="SEX" type="VARCHAR(255)"/>
            <column name="DEPARTMENT" type="VARCHAR(255)"/>
            <column name="VERSION" type="INT">
                <constraints nullable="false"/>
            </column>
            <column name="CREATED_BY" type="VARCHAR(255)"/>
            <column name="CREATED_DATE" type="${offsetDateTime.type}"/>
            <column name="LAST_MODIFIED_BY" type="VARCHAR(255)"/>
            <column name="LAST_MODIFIED_DATE" type="${offsetDateTime.type}"/>
            <column name="DELETED_BY" type="VARCHAR(255)"/>
            <column name="DELETED_DATE" type="${offsetDateTime.type}"/>
        </createTable>
    </changeSet>
</databaseChangeLog>